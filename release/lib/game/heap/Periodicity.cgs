class Periodicity : java "org.cgsuite.lang.impartial.APInfo"

    property Preperiod.get : java "getPreperiod";
    property Period.get : java "getPeriod";
    property Saltus.get : java "getSaltus";
    
    override property ToString.get
        return "Periodicity(Period => " + Period.ToString +
               ", Preperiod => " + Preperiod.ToString +
               ", Saltus => " + Saltus.ToString + ")";
    end

end