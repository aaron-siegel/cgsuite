class InverseGame extends Game : java "org.cgsuite.lang.game.InverseGame"

    property Underlying.get : java "getG";

    method Options(Player player)
        return setof(-g for g in this.Underlying.Options(player.Opponent));
    end
    
    property CanonicalForm.get
        return -this.Underlying.CanonicalForm;
    end

end
