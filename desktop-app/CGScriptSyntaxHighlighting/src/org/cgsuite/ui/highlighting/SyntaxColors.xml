<?xml version="1.0" encoding="UTF-8"?>

<!-- Example of how to vary the style:
    <fontcolor name="Keyword" foreColor="blue" default="default">
        <font style="bold italic"/>
    </fontcolor>
-->

<!DOCTYPE fontscolors PUBLIC "-//NetBeans//DTD Editor Fonts and Colors settings 1.1//EN" "http://www.netbeans.org/dtds/EditorFontsColors-1_1.dtd">
<fontscolors>

    <fontcolor name="Keyword" foreColor="blue" default="default"/>
    <fontcolor name="DeclKeyword" foreColor="ff009900" default="default">
        <font style="bold"/>
    </fontcolor>
    <fontcolor name="String"  foreColor="ffce7b00" default="default"/>
    <fontcolor name="Comment" foreColor="ff969696" default="default"/>
    <fontcolor name="DocComment" foreColor="ff969696" default="default">
        <font style="bold"/>
    </fontcolor>
    <fontcolor name="DeclId"  foreColor="black" default="default">
        <font style="bold"/>
    </fontcolor>
    <fontcolor name="InfixOp"  foreColor="black" default="default">
        <font style="italic"/>
    </fontcolor>

</fontscolors>
