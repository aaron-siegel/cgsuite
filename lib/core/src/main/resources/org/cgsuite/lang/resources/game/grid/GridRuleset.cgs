/*${cgsuite.banner}*/

class GridRuleset extends Ruleset of Grid

  def Eval(str as String) := Eval(Grid.Parse(str, CharMap));

  def CharMap as String := error("Subclasses of `GridRuleset` must override `CharMap`.");

  def Icons as List of Icon := error("Subclasses of `GridRuleset` must override `Icons`.");

  override def MakeOutput(grid as Grid) := cgsuite.util.output.GridOutput(grid, Icons, EnclosingObject.Class.Name + "(\"" + grid.ToString(CharMap) + "\")");

end
