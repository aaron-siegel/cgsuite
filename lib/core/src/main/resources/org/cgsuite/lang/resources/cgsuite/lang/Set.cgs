/*${cgsuite.banner}*/

system class Set extends Collection

  external def Contains(x);

  def Apply(Procedure fn) := setof(fn(x) for x in this);

  def Mex begin
    var n := 0;
    while Contains(n) do
      n := n + 1;
    end
    n
  end

  external def Replaced(Map replacements);

  override def ToOutput := "{" + ToList.Sorted.MkOutput(",") + "}";

end
