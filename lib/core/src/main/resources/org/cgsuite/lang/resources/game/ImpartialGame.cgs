system class ImpartialGame extends Game

  override def CanonicalForm := *NimValue;
  override def GameValue := *NimValue;
  external def Genus;
  override def LeftOptions := Options;
  external def MisereCanonicalForm;
  external def MisereNimValue;
  external def NimValue;
  override def Options := error("Subclasses of `ImpartialGame` must override the `Options` method.");
  override def RightOptions := Options;

end
