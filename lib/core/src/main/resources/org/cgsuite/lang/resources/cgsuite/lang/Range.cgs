/*${cgsuite.banner}*/

system class Range extends Collection

  def Apply(fn as Procedure) := listof(fn(x) for x in this);

  external def Contains(x);

  external def Start;
  external def End;
  external def Step;

  override def ToOutput begin
    if Step == 1 then
      Start.ToOutput + ".." + End.ToOutput
    else
      Start.ToOutput + ".." + End.ToOutput + ".." + Step.ToOutput
    end
  end

end
