/*${cgsuite.banner}*/

system class StopperSidedValue extends NormalValue

  external def Offside;
  external def Onside;

  override def ToOutput begin
    if Onside == on and Offside == off then
      cgsuite.util.output.TextOutput("dud")
    else
      Onside.ToOutput + " & " + Offside.ToOutput
    end
  end

end
