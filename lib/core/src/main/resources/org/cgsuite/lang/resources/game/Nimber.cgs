system class Nimber extends ImpartialGame, Uptimal

  external def op+(that as Nimber) as Nimber;

  external def op-(that as Nimber) as Nimber;

  external def op pos as Nimber;

  external def op neg as Nimber;

  override external def Options;

  override def OptionsFor(player as Player) := Options;

end
