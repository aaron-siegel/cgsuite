class FoxAndGeese extends GridGame

    private static var Icons := [
        GridIcon.Blank,
        GridIcon.BlackStone,
        GridIcon.WhiteStone
        ];

    public method FoxAndGeese(grid)
        if grid.Class == String then
            this.grid := Grid.ParseGrid(grid, ".xo");
        elseif grid.Class == Grid then
            this.grid := grid;
        end
        // TODO Error otherwise
    end

    

end